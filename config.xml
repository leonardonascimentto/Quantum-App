<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.quantum" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>Quantum</name>
	<description>
		Sistema Quantum
	</description>
	<author email="contato@redintel.com.br" href="http://www.redintel.com.br">
	  Redintel
	</author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1.2.1" />
  <plugin name="cordova-plugin-dialogs" spec="1.2.0"/>
  <plugin name="cordova-plugin-vibration" spec="2.1.0" />
  <plugin name="cordova-plugin-file" spec="4.1.1" />
  <plugin name="cordova-plugin-media" spec="2.1.0" />
  <plugin name="cordova-plugin-media-capture" spec="1.2.0" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
	<icon src="icon.png" /> <!-- Define o caminho do ícone padrão 'global' com 128px X 128px, o caminho é em relação à pasta www -->
</widget>